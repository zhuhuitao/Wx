/* searchUser.wxss */
page{
  background: #fff;
}
.search-padd{
  display: flex;
  flex-direction: column;
}
.search-cm{
  display: flex;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #EF6B2B;
}
.search-txt{
  flex: 1;
  height: 80rpx;
  line-height: 80rpx;
}
.search-btn{
  width: 70rpx;
  height: 80rpx;
  background-image: url("data:image/png;base64,");
  background-size: 46rpx;
  background-position: center center;
  background-repeat: no-repeat;
  margin-left: 30rpx;
}
.search-emp{
  padding-top: 60rpx;
  text-align: center;
  color: #666;
}
.search-list{
  padding: 18rpx 0;
}
.search-li{
  border-bottom: 1px solid #DBDBDB;
  padding: 20rpx 0;
  line-height: 1.5;
  font-size: 30rpx;
}
.search-time{
  color: #A3A3A3;
  font-size: 30rpx;
}
.search-name{
  display: flex;
  padding: 6rpx 0;
}
.search-name .tel{
  color: #EF6B2B;
  padding-left: 30rpx;
}
.search-sn{
  display: flex;
}
.sn-bt{
  color: #6F6F6F;
}
.sn-nb{
  text-transform: uppercase;
}